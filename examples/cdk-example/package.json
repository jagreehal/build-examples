{
  "name": "cdk-example",
  "description": "CDK Serverless Example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --skipLibCheck",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy": "cdk deploy -O ./cdk-exports.json",
    "cdk:hot": "cdk deploy --hotswap -O ./cdk-exports.json",
    "cdk:destroy": "cdk destroy",
    "cdk:diff": "cdk diff ",
    "cdk:list": "cdk list",
    "cdk:synth": "cdk synth",
    "cdk:synth:output": "cdk synth --output=./templates",
    "cdk:synth:template": "cdk synth --no-staging > template.yaml",
    "sam:start": "sam local start-api"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.110",
    "aws-cdk-lib": "2.63.2",
    "aws-sdk": "^2.1310.0",
    "constructs": "^10.1.244",
    "@jagsjoint/example-library": "workspace:*"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^2.63.2",
    "@tsconfig/node16": "^1.0.3",
    "@types/aws-lambda": "^8.10.110",
    "@types/eslint": "^8.21.0",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "aws-cdk": "^2.63.2",
    "esbuild": "^0.17.6",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "aws",
    "cdk",
    "typescript"
  ],
  "tags": [
    "aws",
    "cdk",
    "typescript",
    "lambda"
  ],
  "author": "Jag Reehal",
  "license": "MIT"
}
